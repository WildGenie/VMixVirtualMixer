<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:VMix">
    <SolidColorBrush x:Key="Background" Color="White"/>
    <SolidColorBrush x:Key="ControlFill" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="ControlFillMouseOver" Color="White"/>
    <SolidColorBrush x:Key="ControlFillMouseDown" Color="#FFAFBDDC"/>
    <SolidColorBrush x:Key="ControlAccent" Color="#FF00BEF4"/>
    <SolidColorBrush x:Key="Text" Color="Black"/>
    <SolidColorBrush x:Key="TextMouseDown" Color="Black"/>
    <SolidColorBrush x:Key="ControlBorder" Color="#FFB4B4B4"/>
    <SolidColorBrush x:Key="ControlBorderMouseOver" Color="#FF9E9E9E"/>
    <Color x:Key="ControlGradientStop0" A="255" R="235" G="235" B="235"/>
    <Color x:Key="ControlGradientStop1" A="255" R="225" G="225" B="225"/>
</ResourceDictionary>